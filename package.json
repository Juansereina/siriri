{
  "name": "siriri-19",
  "version": "1.0.0",
  "description": "Front",
  "main": "index.js",
  "scripts": {
    "start": "node",
    "dev": "gulp serve & npm run js-w",
    "test": "npm run lint && jest --testPathPattern=spec.js",
    "build": "echo 'Building' && cross-env NODE_ENV=\"production\" gulp build && cross-env NODE_ENV=\"production\" npm run js",
    "js": "rollup -c rollup.config.js",
    "js-w": "rollup -c rollup.config.js -w",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint src",
    "prettier": "prettier -c './**/*.{html,md,js,json}'",
    "prettier:fix": "prettier --write './**/*.{html,md,js,json}'",
    "lighthouse": "npm run pre-lighthouse && node ./helpers/lighthouse",
    "pre-lighthouse": "rm -f lighthouse-report.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Juansereina/siriri-19.git"
  },
  "author": "Juansereina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Juansereina/siriri-19/issues"
  },
  "homepage": "https://github.com/Juansereina/siriri-19#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "browser-sync": "^2.26.7",
    "chrome-launcher": "^0.13.1",
    "cross-env": "^7.0.2",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.2.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "jest": "^25.1.0",
    "js-yaml": "^3.13.1",
    "lighthouse": "^5.6.0",
    "lighthouse-logger": "^1.2.0",
    "prettier": "2.0.2",
    "rollup": "^2.3.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0"
  }
}
